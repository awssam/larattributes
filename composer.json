{
    "name": "awssam/larattributes",
    "description": "Larattributes is a robust, intelligent, and integrated Entity-Attribute-Value model (EAV) implementation for Laravel Eloquent, with powerful underlying for managing entity attributes implicitly as relations with ease. It utilizes the power of Laravel Eloquent, with smooth and seamless integration.",
    "type": "library",
    "keywords": [
        "eav",
        "eloquent",
        "laravel",
        "dynamic",
        "robust",
        "schema",
        "model",
        "value",
        "entity",
        "database",
        "attribute",
        "integrated",
        "intelligent",
        "entity-attribute-value"
    ],
    "license": "MIT",
    "homepage": "https://awtsoft.ma",
    "support": {
        "email": "help@awtsoft.ma",
        "issues": "https://github.com/awssam/larattributes/issues",
        "source": "https://github.com/awssam/larattributes",
        "docs": "https://github.com/awssam/larattributes/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Awssam Saidi",
            "homepage": "https://awtsoft.ma",
            "email": "me@awtsoft.ma"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "illuminate/console": "^8.0.0 || ^9.0.0",
        "illuminate/database": "^8.0.0 || ^9.0.0",
        "illuminate/support": "^8.0.0 || ^9.0.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.31.0",
        "illuminate/container": "^8.0.0 || ^9.0.0",
        "orchestra/testbench": "^6.7.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Larattributes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larattributes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larattributes\\Providers\\AttributesServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
